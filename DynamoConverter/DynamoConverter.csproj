<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0.4</Version>
        <Title>DynamoConverter</Title>
        <Authors>Jose Perez</Authors>
        <Description>A nuget to handle AWS DynamoDb entities</Description>
        <RepositoryUrl>https://github.com/Jr382/DynamoConverter</RepositoryUrl>
        <PackageTags>DynamoDb, Serializer, Deserializer</PackageTags>
        <PackageReleaseNotes>Solved Enumerable deserialization :(</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.100.22" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    </ItemGroup>

</Project>
